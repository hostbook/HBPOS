.toll_free_banner {
	position: fixed;
	top: 15px;
	right: 0px;
	background: #F77B01;
	padding: 0px 0px 0px 0px;
	color: #fff;
	font-size: 24px;
	z-index: 999999;
	border-top-left-radius: 4px;
	border-bottom-left-radius: 4px;
	box-shadow: 0px 0px 5px #353535;
	width: 335px;
}
.call_to_connect {
	width: 40px;
	float: left;
	background: #3e944d;
	height: 34px;
	text-align: center;
	display: block;
}
.toll_number {
	padding-left: 10px;
	float: left;
	padding-right: 10px;
}
.call_to_connect:hover .toll_free_banner {
	-webkit-transition: all 0.4s;
	-moz-transition: all 0.4s;
	transition: all 0.4s;
	right: 0px;
}
.call_to_connect img {
	width: 25px !important;
}
.toll_free_banner {
	-webkit-animation: shift 4000ms 4 alternate;
	-moz-animation: shift 4000ms 4 alternate;
	-o-animation: shift 4000ms 4 alternate;
	animation: shift 4000ms 4 alternate;
}
.right-shift {
	right: -295px;
}
@-webkit-keyframes shift {
 from {
right:-295px;
}
 to {
right:0px;
}
}
@-o-keyframes shift {
 from {
right:-295px;
}
 to {
right:0px;
}
}
@-moz-keyframes shift {
 from {
right:-295px;
}
 to {
right:0px;
}
}
@keyframes shift {
 from {
right:-295px;
}
 to {
right:0px;
}
