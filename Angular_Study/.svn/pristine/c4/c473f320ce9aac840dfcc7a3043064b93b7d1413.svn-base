<h1>Contact Us</h1>
<form method="post">
  <div class="form-group">
    <label>Name</label>
    <input type="text" [(ngModel)]="myData.name" class="form-control" name="name"/>
  </div>
  <div class="form-group">
      <label>Email</label>
      <input type="text" [(ngModel)]="myData.email" class="form-control" name="email"/>
    </div>
    <div class="form-group">
        <label>Mobile</label>
        <input type="text" [(ngModel)]="myData.mobile" class="form-control" name="mobile"/>
      </div>
      <input type="button" value="Send" class="btn btn-success" (click)="Contact()"/>
</form>